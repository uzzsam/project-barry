# MongoDB Connection Strings
MONGODB_URI_PROD=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority
MONGODB_URI_PREVIEW=mongodb+srv://username:password@cluster.mongodb.net/database-preview?retryWrites=true&w=majority

# API Keys
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key

# Wix Authentication
WIX_APP_PUBLIC_KEY=your-wix-app-public-key
WIX_APP_ID=your-wix-app-id

# Pinecone (for RAG implementation)
PINECONE_API_KEY_PROD=your-pinecone-api-key-prod
PINECONE_API_KEY_PREVIEW=your-pinecone-api-key-preview
PINECONE_ENVIRONMENT_PROD=your-pinecone-environment-prod
PINECONE_ENVIRONMENT_PREVIEW=your-pinecone-environment-preview
PINECONE_INDEX_NAME=your-pinecone-index-name

# Vercel Environment Variables
VERCEL_URL=your-vercel-url
VERCEL_ENV=development

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=60
